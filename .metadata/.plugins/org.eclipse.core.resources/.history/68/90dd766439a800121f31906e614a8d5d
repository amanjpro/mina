package scala.ch.usi.inf.l3.mina.store

import scala.reflect.api.Trees
import scala.ch.usi.inf.l3._
import mina._

sealed trait HPEAny

case class HPEObject(val tpe: Class, val store: Environment)

case class HPELiteral(val value: Trees#Literal) {
  val tpe = value.tpe
}